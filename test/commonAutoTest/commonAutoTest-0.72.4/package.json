    "test:ios": "react-native run-ios",
    "test:web": "cd web && webpack serve",
    "web-dev": "cd web && webpack --mode development --progress",
    "web-prod": "cd web && webpack --mode production --progress",
    "start:metro": "adb reverse tcp:8081 tcp:8081 && react-native start --reset-cache",
    "test": "jest",
