    "package-android": "react-native bundle --platform android --dev false --entry-file ../src/index.js --bundle-output ../src/android/app/src/main/assets/index.android.bundle && cd ./android && ./gradlew app:assembleRelease",
    "test:android": "adb reverse tcp:8081 tcp:8081 && cd android && ./gradlew installDebug -PreactNativeDevServerPort=8081 && cd ../ && react-native run-android --verbose",
